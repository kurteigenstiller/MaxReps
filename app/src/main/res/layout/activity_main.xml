<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.einkarat.maxreps.MainActivity">

    <SeekBar
        android:id="@+id/seekBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="275dp"
        android:layout_height="36dp"
        android:max="100"
        android:progress="1"
        android:thumb="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="43dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gewicht geschafft (kg)"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wiederholungen geschafft"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="99dp" />

    <SeekBar
        android:id="@+id/seekBar2"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="271dp"
        android:layout_height="35dp"
        android:max="40"
        android:progress="1"
        android:thumb="1"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="125dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="13dp"
        android:text="Maximalkraft (1 Wiederholung)"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="193dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="68dp"
        android:layout_height="44dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="243dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="68dp"
        android:layout_height="44dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="175dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="Last bei 10 Wiederholungen (kg)"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="251dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:text="Last bei 20 Wiederholungen (kg)"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="68dp"
        android:layout_height="44dp"
        android:text="0"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="304dp" />

</android.support.constraint.ConstraintLayout>
